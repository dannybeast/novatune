extends ../layout/main

block variables
    - var title = 'Мультимедиа для BMW 530d';
    - var description = 'Мультимедиа для BMW 530d';
    - var pageName = 'index';
    - var bodyClass = 'is-page-index'

block content

 +b.SECTION.main-intro

   +e.swiper-container.swiper-container.js-swiper-intro 

    +e.swiper-wrapper.swiper-wrapper 
     
     // Slide
     +e.swiper-slide.swiper-slide
       +b.main-slide 
        .container 
         +e.row 
          +e.content(data-swiper-parallax="-200")
           +e.H2.title Придайте индивидуальности своему автомобилю <span class="color-primary">в novatune</span>
           +e.description 
            p С гарантией качества на срок владения автомобилем. Опыт работы более 15 лет.
          +e.media 
           img(src="assets/images/main-intro/car.png", alt="" data-swiper-parallax="-300")

     // Slide
     +e.swiper-slide.swiper-slide
       +b.main-slide 
        .container 
         +e.row 
          +e.content(data-swiper-parallax="-200")
           +e.H2.title Придайте индивидуальности своему автомобилю <span class="color-primary">в novatune</span>
           +e.description 
            p С гарантией качества на срок владения автомобилем. Опыт работы более 15 лет.
          +e.media 
           img(src="assets/images/main-intro/car.png", alt="" data-swiper-parallax="-300")
    
    .container 
     +b.swiper-nav 
      +e.prev(ref="prev") 
        +icon('arrow')
      +e.next(ref="next")
        +icon('arrow')

 +b.SECTION.why
  .container 
   +e.H3.title 
    span Почему мы?
   +e.row 
    +e.col 
     +b.why-item 
      +e.P.title Гарантия сохранности Вашего автомобиля в нашем техцентре
      +e.description 
       p Автомобили клиентов находятся под охраной  и наблюдением 24/7
      +e.icon 
       +icon('shield')

    +e.col 
     +b.why-item 
      +e.P.title Квалифицированный персонал с опытом работы более 15 лет
      +e.description 
       p Мы трепетно относимся к Вашему автомобилю
      +e.icon 
       +icon('application')
    +e.col
     +b.why-item(class="--primary")
      +e.P.title Точный Онлайн расчет стоимости дооснащения для вашего авто
      +e.bottom 
       +b.A(href="#" class="btn --black --skew --large __btn") 
        span Расчитать стоимость

 include ../includes/modules/_models

 include ../includes/modules/_complex

 +b.SECTION.services
  -
   var services = [
    {
     title: 'Навигационно-мультимедийные системы для штатных мониторов',
     img: 'assets/images/work/2.png'
    },{
     title: 'Расширенный CarPlay и Android Auto в любой авто',
     img: 'assets/images/work/3.png'
    },{
     title: 'Автоматические доводчики дверей',
     img: 'assets/images/work/4.png'
    },{
     title: 'Авто-ателье перетяжка и перешив салона, офис на колесах',
     img: 'assets/images/work/5.png'
    },{
     title: 'Внешний тюнинг, обвесы, карбон',
     img: 'assets/images/work/17.png'
    },{
     title: 'Автосвет, подсветка, диоды, ambient light',
     img: 'assets/images/work/7.png'
    },{
